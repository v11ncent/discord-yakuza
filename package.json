{
  "name": "discord-yakuza",
  "version": "1.0.0",
  "description": "Leaderboard for yakuza ups / downs / trues",
  "scripts": {
    "client": "tsc && node -r dotenv/config ./build/main.js",
    "server": "tsc && node ./build/server/index.js",
    "start": "tsc && node -r dotenv/config ./build/main.js && node ./build/server/index.js",
    "deploy": "node -r dotenv/config ./build/deploy.js",
    "watch": "tsc --watch",
    "build": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mongodb": "^6.9.0",
    "mongoose": "^8.9.6",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "3.3.3",
    "typescript": "^5.6.3"
  }
}
